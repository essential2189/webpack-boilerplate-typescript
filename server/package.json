{
  "name": "javascript-p3-fleamarket",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "env $(cat .env | grep -v '#' | xargs) nodemon --watch '**/*.ts' --exec 'ts-node' app.ts",
    "mockup": "mysql -u $(cat .env | grep 'MYSQL_USER=' | sed 's/^.*=//') -p < ../fleamarket.sql && env $(cat .env | grep -v '#' | xargs) ts-node db/mockup.ts"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.2",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "package.json": "^0.0.0",
    "rand-token": "^1.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "8.22.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-sql": "^0.12.1"
  }
}
